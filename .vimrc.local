nnoremap j jzz
nnoremap <Down> jzz
nnoremap k kzz
nnoremap <Up> kzz
nnoremap n nzz
nnoremap N Nzz
nnoremap G Gzz
nnoremap <C-a> ggVG
nnoremap \| V$%
nnoremap <F3> :%s#\($\n\s*\)\+\%$##<CR>
nnoremap <F4> :%s/\(^\s*$\n\)\{2,\}/\r/g<CR>
nnoremap <F5> :%s/\s\+$//e<CR>
nnoremap <C-m> :e ++ff=dos
vnoremap \| $%

let g:minimap_highlight='MoreMsg'

" emmet-vim settings
let g:user_emmet_mode='a'    "enable all function in all mode.
let g:user_emmet_expandabbr_key='<Tab>'
